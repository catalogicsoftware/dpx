var rDay = row["name"].split("-")&#13;
var today = new Date();&#13;
today.setHours(0,0,0,0);&#13;
var yyyy = rDay[0];&#13;
var mm = rDay[1]-1;&#13;
var dd = rDay[2]; &#13;
var reportDay = new Date(yyyy,mm,dd);&#13;
var diffDays =  today.getTime()-reportDay.getTime()&#13;
diffDays = Math.round(diffDays/oneDay)&#13;
//diffDays = (diffDays/oneDay)&#13;
//diffDays = Math.round(Math.abs((today.getTime() - reportDay.getTime())/(oneDay)));&#13;
//BirtDateTime.diffDay(today, reportDay2)&#13;
diffDays+1</expression>
